<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd" version="2.1">

	<tlib-version>1.3</tlib-version>
	<short-name>feilonghtml</short-name>
	<description>飞龙java自定义html标签</description>
	<uri>http://java.feilong.com/tags-html</uri>

	<!--通用html标签-->
	<tag>
		<name>html</name>
		<tag-class>com.feilong.taglib.html.HtmlTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>tagType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>标签类型 div span 默认 div</description>
		</attribute>
		<attribute>
			<name>toDecrypt</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>需不需要解密 默认false</description>
		</attribute>
		<attribute>
			<name>styleId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>标签id</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>style</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>title</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>classname</description>
		</attribute>

	</tag>

	<!--radio标签-->
	<tag>
		<name>htmlRadio</name>
		<tag-class>com.feilong.taglib.html.HtmlRadioTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>styleId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>标签id</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>style</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>title</description>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>classname</description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>valueAndTexts</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>scopeOptions</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>scopeLabel</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>scopeValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[优先级顺序:value paramNameValue defaultValue]]></description>
		</attribute>
		<attribute>
			<name>defaultValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[默认选中,当不存在参数且没有value的时候 该选项选中]]></description>
		</attribute>
	</tag>

	<!-- image 图片不失真-->
	<tag>
		<name>htmlImage</name>
		<tag-class>com.feilong.taglib.html.HtmlImageTag</tag-class>
		<body-content>empty</body-content>
		<description><![CDATA[图片不失真]]></description>
		<attribute>
			<name>styleId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[Id]]></description>
		</attribute>
		<attribute>
