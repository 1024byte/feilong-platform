		<property name="skuCostMaintainWriter" ref="skuCostMaintainWriter" />
		<property name="cashCardExportTemplateWriter" ref="cashCardExportTemplateWriter"></property>
		<property name="spUploadGroupRefresWriter" ref="spUploadGroupRefresWriter" />
		<property name="memberCardMaintainWriter" ref="memberCardMaintainWriter" />
		<property name="saleCompleteWriter" ref="saleCompleteWriter" />
		<property name="saleSoByMonthWriter" ref="saleSoByMonthWriter" />
		<property name="soReturnReportWriter" ref="soReturnReportWriter" />
		<property name="skuNoStorageWriter" ref="skuNoStorageWriter"></property>
		<property name="soRepertoryStatWriter" ref="soRepertoryStatWriter" />
		<property name="soReconciliationsWriter" ref="soReconciliationsWriter" />
		<property name="soPrepaidCardWriter" ref="soPrepaidCardWriter" />
		<property name="soRepertoryInfoWriter" ref="soRepertoryInfoWriter" />
