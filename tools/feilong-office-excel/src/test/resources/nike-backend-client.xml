		<property name="soInfoReportWriter" ref="soInfoReportWriter" />
		<property name="commentInfoReportWriter" ref="commentInfoReportWriter" />
		<property name="skuInventorySnapshotWriter" ref="skuInventorySnapshotWriter" />
		<!--**********************销售报表******************-->
		<property name="dailySalesReportWriter" ref="dailySalesReportWriter" />
		<!--**********************trackIngCode报表******************-->
		<property name="salesTrackingCodeWriter" ref="salesTrackingCodeWriter" />
		<!--**********************到货通知报表******************-->
		<property name="memberReplenishmentWriter" ref="memberReplenishmentWriter" />
		<!--**********************邮件发送数量统计报表******************-->
		<property name="mailLogSendTotalWriter" ref="mailLogSendTotalWriter" />

		<!--**********************同步库存日志******************-->
		<property name="syscInventoryWriter" ref="syscInventoryWriter" />
		<!--**********************COD报表导出******************-->
		<property name="codInfoReport" ref="codInfoReport" />
		<!--**********************物流仓储KPI报表******************-->
		<property name="kpiReportWriter" ref="kpiReportWriter" />
	</bean>

	<!--同步库存日志-->
	<bean id="syscInventoryWriter" factory-bean="excelManipulatorFactory" factory-method="createExcelWriter">
		<constructor-arg>
			<list>
				<!--概述-->
				<value>syscInventorySummaryDownload</value>
				<!--未同步到库存OMS商品-->
				<value>syscInventoryOMSYesWMSNoDownload</value>
				<!--WMS存在,OMS库存不存在的upc-->
				<value>syscInventoryWmsYesOmsNoDownload</value>
				<!--wms为0,oms已上架sku code-->
				<value>syscInventoryWmsZeroOmsSellingDownload</value>

			</list>
		</constructor-arg>
	</bean>

	<!--**********************销售报表******************-->
	<bean id="dailySalesReportWriter" factory-bean="excelManipulatorFactory" factory-method="createExcelWriter">
		<constructor-arg>
			<value>exportDailySalesReport</value>
		</constructor-arg>
	</bean>


	<!--**********************trackIngCode报表******************-->
	<bean id="salesTrackingCodeWriter" factory-bean="excelManipulatorFactory" factory-method="createExcelWriter">
