<?xml version="1.0" encoding="UTF-8"?>
<sheets xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="exceltemplate-1.0.xsd">

	<!-- 培训人员名单 -->
	<sheet name="trainSignUpEmployeeSheet">

		<block start="A2" end="F2" dataName="trainSignUpEmployeeEntityList" loop="true" loopClass="com.feilong.project.train.entity.TrainSignUpEmployeeEntity">
			<cell position="A2" dataName="name" />
			<cell position="B2" dataName="email" />
			<cell position="C2" dataName="mobile" />
			<!-- 商城分类 -->
			<cell position="D2" dataName="storeCategoryName" />
			<!-- 商城名称 -->
			<cell position="E2" dataName="storeName" />
		</block>
	</sheet>

	<!-- 报名名单 -->
	<sheet name="trainSignUpSheet">

		<block start="A2" end="F2" dataName="trainSignUpEntitylist" loop="true" loopClass="com.feilong.project.train.entity.TrainSignUpEntity">
			<cell position="A2" dataName="name" />
			<cell position="B2" dataName="signUpTime" />
			<!-- type="date" pattern="yyyy-MM-dd HH:mm:ss" -->
			<cell position="C2" dataName="courseTime" />
			<cell position="D2" dataName="courseName" />
			<!-- 是否是编外人员 -->
			<cell position="E2" dataName="offStaff" />
			<cell position="F2" dataName="remark" />


			<!-- ************************************************* -->
			<!-- 签到状态 -->
			<cell position="G2" dataName="signStatus" />

			<!-- 会议通知 -->
			<cell position="H2" dataName="meetingNoticeScore" />
			<!-- 培训时间安排 -->
			<cell position="I2" dataName="meetingTimeScore" />

			<!-- 培训组织者态度 -->
			<cell position="J2" dataName="trainOrganizersAttitudeScore" />
			<!-- 培训教室布置 -->
			<cell position="K2" dataName="trainRoomLayoutScore" />
			<!-- 现场次序维护 -->
			<cell position="L2" dataName="sceneOrderMaintenanceScore" />
			<!-- 表达清晰态度友善 -->
			<cell position="M2" dataName="clearExpressionFriendlyScore" />
			<!-- 对授课时间的掌控度 -->
			<cell position="N2" dataName="teachTimeDegreeControlScore" />
			<!-- 现场气氛调节能力 -->
			<cell position="O2" dataName="sceneAtmosphereAdjustmentAbilityScore" />
			<!-- PPT内容 -->
			<cell position="P2" dataName="pptContentScore" />
			<!-- PPT设计 -->
			<cell position="Q2" dataName="pptDesignScore" />
			<!-- 对我有用 -->
			<cell position="R2" dataName="usefulToMeScore" />
			<!--干货 -->
			<cell position="S2" dataName="ganhuoScore" />
			<!--是否满意老师 -->
			<cell position="T2" dataName="satisfiedTeacherScore" />

			<!--评价类型 -->
			<cell position="U2" dataName="evaluationType" />
			<!--评价内容 -->
			<cell position="V2" dataName="evaluationContent" />
			<!--沟通结果 -->
			<cell position="W2" dataName="communicateResults" />
		</block>
	</sheet>

	<!-- 签到表 -->
	<sheet name="attendanceSheet">
		<block start="A2" end="G3">
			<!-- 课程名称 -->
			<cell position="C2" dataName="courseName" />

			<!-- 培训日期 -->
			<cell position="F2" dataName="trainDate" />
			<!-- 培训讲师 -->
			<cell position="C3" dataName="trainInstructor" />
			<!-- 培训地点 -->
			<cell position="F3" dataName="trainAddress" />
		</block>

		<block start="A5" end="G5" dataName="attendanceList" loop="true" loopClass="com.feilong.project.train.entity.AttendanceEntity">
			<!-- No -->
			<cell position="A5" dataName="id" />
			<!-- 姓名 -->
			<cell position="B5" dataName="name" />

			<!-- 部门 -->
			<cell position="C5" dataName="departName" />

			<!-- 组别 -->
			<cell position="D5" dataName="groupName" />

			<!-- 会前通知参加 -->
			<cell position="E5" dataName="notice" />

			<!-- 培训签到 -->
			<cell position="F5" dataName="attendanceSign" />

			<!-- 备注 -->
			<cell position="G5" dataName="remark" />
		</block>
	</sheet>
</sheets>